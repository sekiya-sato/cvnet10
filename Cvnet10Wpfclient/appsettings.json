{ /* 環境変数 DOTNET_ENVIRONMENT = Development のときに読み込まれる設定ファイル */
  "ConnectionStrings": {
    "Url": "https://localhost:5012",
    "url_local": "https://localhost:5012",
    "url_jen2": "https://jenkins2.dtpnet.co.jp",
    "url_jen2second": "https://jenkins2.dtpnet.co.jp/cv10second/"
  },
  "Parameters": {
    "Count": 10000,
    "ENV": "Development",
    "LoginId": "123",
    "LoginPass": "123"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning"
    }
  },
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": true,
    "internalLogLevel": "Info",
    "internalLogFile": "${basedir}/logs/internal-nlog.txt",
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "${basedir}/logs/cvnet10client.log",
        "layout": "${longdate}|${level:uppercase=true}|${logger}|${message} ${exception:format=tostring}",
        "createDirs": true,
        "archiveFileName": "${basedir}/logs/archives/cvnet10client-{###}.log",
        "archiveAboveSize": 2048000,
        "archiveNumbering": "Sequence",
        "maxArchiveFiles": 100,
        "encoding": "UTF-8"
      },
      "logconsole": {
        "type": "Console",
        "layout": "${time}|${level:uppercase=true}|${message}"
      }
    },
    "rules": [
      {
        "logger": "Microsoft.*",
        "maxLevel": "Info",
        "final": true
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logfile,logconsole"
      }
    ]
  }
}