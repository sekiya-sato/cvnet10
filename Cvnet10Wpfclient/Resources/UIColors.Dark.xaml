<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Background Colors -->
    <Color x:Key="primaryBackColor1">#1E1E1E</Color>
    <Color x:Key="primaryBackColor2">#121212</Color>
    <Color x:Key="secondaryBackColor1">#252525</Color>
    <Color x:Key="secondaryBackColor2">#1A1A1A</Color>

    <!-- Window Border Colors -->
    <Color x:Key="winBorderColor1">#5C5CFF</Color>
    <Color x:Key="winBorderColor2">#8A5CFF</Color>
    <Color x:Key="winBorderColor3">#9C6BFF</Color>

    <!-- Color Palette -->
    <SolidColorBrush x:Key="color1" Color="#3A2F45"/>
    <SolidColorBrush x:Key="color2" Color="#5D4B9E"/>
    <SolidColorBrush x:Key="color3" Color="#2C7A7B"/>
    <SolidColorBrush x:Key="color4" Color="#B04C7B"/>
    <SolidColorBrush x:Key="color5" Color="#5B5E9C"/>
    <SolidColorBrush x:Key="color6" Color="#C48B3A"/>
    <SolidColorBrush x:Key="color7" Color="#9E4A66"/>
    <SolidColorBrush x:Key="color8" Color="#5B7AA8"/>
    <SolidColorBrush x:Key="color9" Color="#2FAF93"/>
    <SolidColorBrush x:Key="color10" Color="#7A4B52"/>
    <SolidColorBrush x:Key="color11" Color="#3A7D3C"/>
    <SolidColorBrush x:Key="color12" Color="#2B4C7E"/>
    <SolidColorBrush x:Key="color13" Color="#1C2E50"/>

    <!-- Font Colors -->
    <SolidColorBrush x:Key="TitleColor" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="SubTitleColor" Color="#E0E0E0"/>
    <SolidColorBrush x:Key="TitleLabelColor" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="plainTextColor0" Color="#E0E0E0"/>

    <!-- Panel Colors -->
    <SolidColorBrush x:Key="panelColor" Color="#2B2B2B"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#343434"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#3F3F3F"/>

    <!-- Button Colors -->
    <SolidColorBrush x:Key="buttonColor1" Color="#3D6EF7"/>

    <!-- Border Color -->
    <SolidColorBrush x:Key="borderColor" Color="#CCCCCC"/>
</ResourceDictionary>
